<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.berlintourguide">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sport"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tv_tower"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".History"
            android:label="History"/>
        <activity android:name=".OutDoor"
            android:label="Out Door"/>
        <activity android:name=".Dinner"
            android:label="Dinner"/>
        <activity android:name=".Sport"
            android:label="Sport"></activity>
    </application>

</manifest>